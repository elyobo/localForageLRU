{
  "name": "localForageLRU",
  "version": "1.0.0",
  "description": "A wrapper around localForage that provides an LRU cache behavior.",
  "main": "src/localforagelru.js",
  "devDependencies": {
    "karma": "~0.12.16",
    "karma-chrome-launcher": "~0.1.3",
    "karma-coverage": "~0.1",
    "karma-jasmine": "~0.3.4",
    "jasmine-core": "~2.3.4",
    "karma-junit-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "~0.1",
    "karma-spec-reporter": "0.0.12",
    "mocha": "2.2.4"
  },
  "scripts": {
    "clean": "node_modules/.bin/bower cache clean; rm -rf node_modules bower_components",
    "postinstall": "node_modules/.bin/bower install --config.interactive=false",
    "test": "node_modules/.bin/karma start test/karma.conf.js --reporters dots",
    "testonce": "node_modules/.bin/karma start test/karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshvillbrandt/localForageLRU.git"
